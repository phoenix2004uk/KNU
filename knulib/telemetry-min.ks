{export(Lex("version","1.1.0","availableThrust",{RETURN SHIP:AVAILABLETHRUST*1e3.},"currentISP",{LIST ENGINES IN A.LOCAL B IS 0.LOCAL D IS 0.FOR F IN A IF F:IGNITION AND NOT F:FLAMEOUT{SET B TO B+F:AVAILABLETHRUST*1e3.SET D TO D+F:AVAILABLETHRUST*1e3/F:ISP.}IF D=0 RETURN 0.RETURN B/D.},"timeToImpact",{PARAMETER m.LOCAL g IS BODY:MU/BODY:RADIUS^2. LOCAL u IS SHIP:VERTICALSPEED. LOCAL d IS ALT:RADAR-m. RETURN(SQRT(u^2+2*g*d)+u)/g.})).}