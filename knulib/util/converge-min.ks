{LOCAL Y IS import("util/ish").export(Lex("version","1.0.6","create",{PARAMETER A,B,D,F IS "value".RETURN List(0,B,D,F,(B-A)/ABS(B-A),ROUND(A,5),0).},"check",{PARAMETER A,B.SET B TO ROUND(B,5).LOCAL D IS B-A[5].IF D=0 RETURN 0.IF A[4]>0{IF D<0 RETURN 1.}ELSE IF A[4]<0{IF D>0 RETURN 1.}ELSE{IF ABS(D-A[6])>MAX(ABS(D),ABS(A[6]))RETURN 1.}SET A[5]TO B. SET A[6]TO D. RETURN Y[A[3]](B,A[1],A[2]).})).}