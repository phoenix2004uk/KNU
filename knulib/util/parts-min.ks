{LOCAL Z IS{PARAMETER A,B IS 0.LOCAL D IS SHIP:PartsDubbed(A).IF D:LENGTH>B RETURN D[B].RETURN FALSE.}.LOCAL Y IS{PARAMETER A,F,B IS 0.LOCAL H IS Z(A,B).IF JF(H)RETURN FALSE. RETURN H:GetModule(F).}.export(Lex("version","1.1.1","GetPart",Z,"GetPartModule",Y,"DoPartModuleEvent",{PARAMETER A,F,J,B IS 0.LOCAL K IS Y(A,F,B).IF JF(K)OR NOT K:HASEVENT(J)RETURN FALSE. K:DoEvent(J).RETURN TRUE.},"DoPartModuleAction",{PARAMETER A,F,J,L IS TRUE,B IS 0.LOCAL K IS Y(A,F,B).IF JF(K) OR NOT K:HASACTION(J)RETURN FALSE. K:DoAction(J,L).RETURN TRUE.},"GetPartModuleField",{PARAMETER A,F,J,B IS 0.LOCAL K IS Y(A,F,B).IF JF(K) OR NOT K:HASFIELD(J)RETURN FALSE. RETURN K:GetField(J).},"SetPartModuleField",{PARAMETER A,F,J,L,B IS 0.LOCAL K IS Y(A,F,B).IF JF(K) OR NOT K:HASFIELD(J)RETURN FALSE. K:SetField(J,L).RETURN TRUE.},"DoModuleEvent",{PARAMETER A,B.FOR D IN SHIP:MODULESNAMED(A)IF D:HASEVENT(B)D:DoEvent(B).},"DoModuleAction",{PARAMETER A, B, D IS TRUE. FOR F IN SHIP:MODULESNAMED(A)IF F:HASACTION(B)F:DoAction(B, D).})).}